<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
		<g:DockPanel width="100%" height="100%">
			<g:Dock direction="SOUTH" horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_BOTTOM">
				<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE" spacing="5" ui:field="buttonPanel">
					<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE" width="80px">
						<g:Button text="Create" width="80px" ui:field="finishButton"/>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE" width="80px">
						<g:Button text="Cancel" width="80px" ui:field="cancelButton"/>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Dock>
			<g:Dock direction="CENTER" verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER" width="100%" height="100%">
				<g:SimplePanel width="100%" height="100%" ui:field="contentPanel"/>
			</g:Dock>
			<g:Dock direction="NORTH" width="100%" height="">
				<g:HorizontalPanel width="100%" ui:field="titlePanel"/>
			</g:Dock>
		</g:DockPanel>
</ui:UiBinder> 